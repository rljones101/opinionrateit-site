{
  "name": "opinionrateit-site",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "watch-tailwind-changes": "npx tailwindcss -i ./src/assets/main.css -o ./src/assets/styles.css --watch",
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "backend:dev": "cd ./amplify/backend/function/reviewersFn/src/ && SET NODE_ENV=development&&nodemon app.js",
    "backend:install": "npm install --prefix ./amplify/backend/function/reviewersFn/src",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@heroicons/vue": "^2.0.18",
    "axios": "^1.4.0",
    "chart.js": "^3.9.1",
    "flowbite": "^1.6.5",
    "luxon": "^3.3.0",
    "pinia": "^2.0.36",
    "tailwind-scrollbar": "^3.0.4",
    "vue": "^3.3.2",
    "vue-chart-3": "^3.1.8",
    "vue-router": "^4.2.0",
    "vue-youtube-vue-3": "^1.5.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/luxon": "^3.3.0",
    "@types/node": "^18.16.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "eslint": "^8.39.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-vue": "^9.11.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.0.4",
    "vite": "^4.3.5",
    "vue-tsc": "^1.6.4"
  }
}
