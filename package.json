{
  "name": "opinionrateit-site",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "watch-tailwind-changes": "npx tailwindcss -i ./src/assets/main.css -o ./src/assets/styles.css --watch",
    "dev": "npm run server",
    "prod": "npm run build && npm run server:prod",
    "build": "run-p type-check build-only server",
    "preview": "vite preview",
    "build-only": "vite build",
    "server": "ts-node ./server/index.ts",
    "server:prod": "cross-env NODE_ENV=production ts-node ./server/index.ts",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "chart.js": "^3.9.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "luxon": "^3.3.0",
    "mongoose": "^7.2.1",
    "node-fetch": "^3.3.1",
    "pinia": "^2.0.36",
    "sirv": "^2.0.3",
    "ts-node": "^10.9.1",
    "vite-plugin-ssr": "^0.4.130",
    "vue": "^3.3.2",
    "vue-chart-3": "^3.1.8",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/luxon": "^3.3.0",
    "@types/node": "^18.16.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.39.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-vue": "^9.11.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.0.4",
    "vite": "^4.3.5",
    "vue-tsc": "^1.6.4"
  }
}
